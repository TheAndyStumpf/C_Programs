// This program will calculate the sum of 10 integers.
// Developer: Andrew Stumpf
// Date: 4 February 2018
     
#include <stdio.h>
#include <stdlib.h>

int main ()	{

  /* variable definition: */
  int value0,value1,value2,value3,value4,value5,value6,value7,value8,value9,sum;
     
  /* Initialize sum */
  sum = 0;
     
  printf("Enter an Integer for value0\n");
  scanf("%d", &value0);
  printf("Enter an Integer for value1\n");
  scanf("%d", &value1);
  printf("Enter an Integer for value2\n");
  scanf("%d", &value2);
  printf("Enter an Integer for value3\n");
  scanf("%d", &value3);
  printf("Enter an Integer for value4\n");
  scanf("%d", &value4);
  printf("Enter an Integer for value5\n");
  scanf("%d", &value5);
  printf("Enter an Integer for value6\n");
  scanf("%d", &value6);
  printf("Enter an Integer for value7\n");
  scanf("%d", &value7);
  printf("Enter an Integer for value8\n");
  scanf("%d", &value8);
  printf("Enter an Integer for value9\n");
  scanf("%d", &value9);
     
  sum = value0 + value1 + value2 + value3 + value4 + value5 + value6 + value7 + value8 + value9;
  printf("Sum is %d\n " , sum );
     
  if (sum >100)
    printf("Sum is over 100\n");
  if (sum <0)
   	printf("Sum is negative\n");
  return 0;
}
